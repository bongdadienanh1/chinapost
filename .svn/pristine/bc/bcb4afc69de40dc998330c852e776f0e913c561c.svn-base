        FROM
        ysh_custom_ucion_history
        LEFT JOIN ysh_enterprise_info ON ysh_custom_ucion_history.enterprise_id = ysh_enterprise_info.enterprise_id
        LEFT JOIN np_order ON ysh_custom_ucion_history.order_id = np_order.order_id
        LEFT JOIN np_back_order ON ysh_custom_ucion_history.order_id = np_back_order.order_id
        WHERE
        ysh_custom_ucion_history.customer_id = #{customerId,jdbcType=BIGINT}
        GROUP BY
        ysh_custom_ucion_history.order_id,
        ysh_custom_ucion_history.type
        HAVING
        COUNT(
