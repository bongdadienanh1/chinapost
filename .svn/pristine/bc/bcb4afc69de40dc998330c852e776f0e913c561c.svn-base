    <select id="selectDetailByPrimaryKey" resultMap="CustomerUcoinHistoryDetailMap" parameterType="java.lang.Long">
        select
        <include refid="Base_Column_List"/>
        ,(select GROUP_CONCAT(goods_info_name) from np_order_goods where order_id =ysh_custom_ucion_history.order_id)
        goodsInfoNames,
        (select enterprise_name from ysh_enterprise_info where enterprise_id = ysh_custom_ucion_history.enterprise_id)
        accountName,
        (select img_url from ysh_enterprise_info where enterprise_id = (select id from ysh_enterprise_func where
        parent_id is null limit 0,1)) enterpriseLogo
        from ysh_custom_ucion_history
        where ysh_custom_ucion_history.id = #{id,jdbcType=BIGINT}
    </select>

    <!--按条件查询历史详情-->
    <select id="selectDetailByCondition" resultMap="CustomerUcoinHistoryDetailMap">
        select
        <include refid="Base_Column_List"/>
        ,(select GROUP_CONCAT(goods_info_name) from np_order_goods where order_id =ysh_custom_ucion_history.order_id)
        goodsInfoNames,
        (select enterprise_name from ysh_enterprise_info where enterprise_id = ysh_custom_ucion_history.enterprise_id)
        accountName,
        (select img_url from ysh_enterprise_info where enterprise_id = (select id from ysh_enterprise_func where
        parent_id is null limit 0,1)) enterpriseLogo,
        (SELECT customer_remark from np_order where np_order.order_id = ysh_custom_ucion_history.order_id) customerRemark
        from ysh_custom_ucion_history
        where
        <if test="customerId != null">
            ysh_custom_ucion_history.customer_id = #{customerId,jdbcType=BIGINT}
        </if>
        <if test="batchId != null">
            and ysh_custom_ucion_history.batch_id = #{batchId,jdbcType=BIGINT}
        </if>
        <if test="type != null">
            and ysh_custom_ucion_history.type = #{type,jdbcType=VARCHAR}
        </if>
        <if test="orderId != null">
            and ysh_custom_ucion_history.order_id = #{orderId,jdbcType=BIGINT}
