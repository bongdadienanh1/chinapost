package com.ylife.exception;

/**
 * Created by InThEnd on 2016/4/11.
 * 客户访问记录异常。
 */
public class AccessRecordException extends RuntimeException {

    public static final String message = "记录访问错误。";

    public AccessRecordException() {
        super(message);
    }

    public AccessRecordException(String message) {
        super(message);
    }

    public AccessRecordException(String message, Throwable cause) {
        super(message, cause);
    }

    public AccessRecordException(Throwable cause) {
        super(cause);
    }

}
