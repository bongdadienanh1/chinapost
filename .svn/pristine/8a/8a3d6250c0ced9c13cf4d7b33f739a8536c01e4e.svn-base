package com.ylife.form.model;

import com.ylife.inventory.model.InventoryHistory;

import java.util.List;

/**
 * Created by XiaoBiaoGe on 2016/11/21.
 */
public class InventoryHistoryForm extends InventoryHistory{

    public InventoryHistoryForm() {}

    public InventoryHistoryForm(String goodsInfoName, String enterpriseName,String goodsSpecString,Integer lastMonthInventory,String goodsInfoItemNo)  {
        this.goodsInfoName = goodsInfoName;
        this.enterpriseName=enterpriseName;
        this.goodsSpecString = goodsSpecString;
        this.lastMonthInventory=lastMonthInventory;
        this.goodsInfoItemNo=goodsInfoItemNo;
    }

    private String sumFlag;

    private Integer lastMonthInventory;

    private String goodsInfoName;

    private String goodsSpecString;

    private String enterpriseName;

    private String goodsInfoItemNo;

    public String getSumFlag() {
        return sumFlag;
    }

    public void setSumFlag(String sumFlag) {
        this.sumFlag = sumFlag;
    }

    public String getGoodsInfoItemNo() {
        return goodsInfoItemNo;
    }

    public void setGoodsInfoItemNo(String goodsInfoItemNo) {
        this.goodsInfoItemNo = goodsInfoItemNo;
    }

    public String getEnterpriseName() {
        return enterpriseName;
    }

    public void setEnterpriseName(String enterpriseName) {
        this.enterpriseName = enterpriseName;
    }

    public String getGoodsInfoName() {
        return goodsInfoName;
    }

    public void setGoodsInfoName(String goodsInfoName) {
        this.goodsInfoName = goodsInfoName;
    }

    public String getGoodsSpecString() {
        return goodsSpecString;
    }

    public void setGoodsSpecString(String goodsSpecString) {
        this.goodsSpecString = goodsSpecString;
    }

    public Integer getLastMonthInventory() {
        return lastMonthInventory;
    }

    public void setLastMonthInventory(Integer lastMonthInventory) {
        this.lastMonthInventory = lastMonthInventory;
    }
}
