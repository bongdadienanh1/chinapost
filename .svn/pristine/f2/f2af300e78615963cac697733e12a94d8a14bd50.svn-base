            <#--<td>¥${(order.orderPrice)?string("0.00")}</td>-->
            <#--<td>${order.mallName}</td>-->
            <#--<td>-->
            <#--<div class="btn-group" style="width: 112px;">-->
            <#--<button href="">查看详情</button>-->
            <#--</div>-->
            <#--</td>-->
            <#--<#else >-->
            <#--<td></td>-->
            <#--<td>${order.mallName}</td>-->
            <#--<td>-->
            <#--<div class="btn-group">-->
            <#--<#if order.orderStatus=="1"&&order.orderLinePay=="1">-->
            <#--<button type="button" class="btn btn-primary btn-sm" style="width: 112px;" onclick="javascript:window.location.href='querythirdoutstock.html?n=9&l=59'">去拣货</button>-->
            <#--<#else>-->
            <#--<button  class="btn btn-primary btn-sm btn_css"  style="width: 112px;">暂无操作</button>-->
            <#--</#if>-->
            <#--</div>-->
            <#--</td>-->
            <#--</#if>-->
            <#--</tr>-->
            <#--</tbody>-->
            <#--</#list>-->
                <tbody>
                <tr class="separation-row">
                    <td colspan="8"></td>
                </tr>
                <tr class="header-row">
                    <td colspan="6">
                        <p>订单号：111111111</p>
                    </td>
                    <td class="row-ops" colspan="2">

                    </td>
                </tr>
                <tr class="content-row">
                    <td class="order-pros">
                    <#--遍历该订单下面的所有商品的信息-->
                        <a href="javascript:;" title=""><img src="" alt="商品1" width="60" height="60"/></a>
                    </td>
                    <td>
                        （1111件）
                    </td>
                    <td style="line-height: 20px;">买家1<br/>18146686909</td>
                    <td>2017-05-0110:44:12:11</td>
                    <td>
                        已付款
                    </td>
                <#--判断是店铺入驻商城还是供货商入驻商城的订单-->
                    <td>¥100</td>
                    <td>所属商城1</td>
                    <td>
                        <div class="btn-group" style="width: 112px;">
                            <button  class="btn btn-primary btn-sm btn_css"  style="width: 112px;"> <a href="orderListDetail.ftl" style="color: #ffffff">查看详情</a></button>
                        </div>
                    </td>

                </tr>
                </tbody>
            </table>
            <div class="footer-operation">
                <div class="ops-left">

                </div>
                <div class="ops-right">
                    <nav>
                        <ul class="pagination">
                            <li>
                                <a href="javascript:;" aria-label="Previous" onclick="changePageNo(this);" data-role="${map.pb.prePageNo}">
                                    <span aria-hidden="true">&laquo;</span>
                                </a>
                            </li>
                        <#if (map.pb.startNo?number>1)>
                            <li><a href="javascript:;" onclick="changePageNo(this);" data-role="1">1</a></li>
                        </#if>
                        <#list map.pb.startNo?number .. map.pb.endNo as item>
                            <li <#if item == map.pb.pageNo>class="active"</#if>><a href="javascript:;" onclick="changePageNo(this);" data-role="${item}">${item}</a></li>
                        </#list>
                        <#if (map.pb.totalPages?number>map.pb.endNo)>
                            <li><a href="javascript:;" onclick="changePageNo(this);" data-role="${map.pb.totalPages}">${pageBean.totalPages}</a></li>
                        </#if>
                            <li>
                                <a href="javascript:;" aria-label="Next" onclick="changePageNo(this);" data-role="${map.pb.nextPageNo}">
                                    <span aria-hidden="true">&raquo;</span>
                                </a>
                            </li>
                        </ul>
                    </nav>
                </div>
            </div>
        <#--<#else>-->
        <#--<tfoot>-->
        <#--<tr>-->
        <#--<td colspan="12">暂无信息</td>-->
        <#--</tr>-->
        <#--</tfoot>-->
        <#--</#if>-->
        </form>
    </div>










</div>
</div>
</div>
<#--</form>-->
<#include "common/leftfooter.ftl">


