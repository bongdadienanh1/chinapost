public enum CreditOrderReason {

    NOT_WANT_BUY("不想买了"),
    WRONG_INFORMATION("收货人信息有误"),
    GOODS_DAMAGE("商品损坏"),
    GOODS_DESC_ERROR("收到商品与描述不符"),
    NOT_DELIVERY_TIMED("未按指定时间发货"),
    OTHER("其他");

    private String tag;

    CreditOrderReason(String tag) {
        this.tag = tag;
    }


    public String getTag() {
        return tag;
    }

    @Override
    public String toString() {
        return tag ;
    }
}
