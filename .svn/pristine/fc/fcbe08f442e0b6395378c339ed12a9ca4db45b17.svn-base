package com.ylife.cache.exception;

/**
 * Created by InThEnd on 2017/2/20.
 */
public class CacheAnnotationParseException extends CacheException {

    public static final String message = "缓存注解解析错误。";

    public CacheAnnotationParseException() {
        super(message);
    }

    public CacheAnnotationParseException(String message) {
        super(message);
    }

    public CacheAnnotationParseException(String message, Throwable cause) {
        super(message, cause);
    }

    public CacheAnnotationParseException(Throwable cause) {
        super(cause);
    }

}
