
<#assign bath = request.contextPath>
<!DOCTYPE html>
<html lang="zh-cn">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title></title>

    <!-- Bootstrap -->
    <link href="${bath}/static/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link rel="stylesheet" href="${bath}/static/css/font-awesome.min.css">
    <link href="${bath}/static/iconfont/iconfont.css" rel="stylesheet">
    <link href="${bath}/static/css/summernote.css" rel="stylesheet">
    <link href="${bath}/static/bootstrap/css/bootstrap-select.min.css" rel="stylesheet">
    <link href="${bath}/static/css/style.css" rel="stylesheet">

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="http://cdn.bootcss.com/html5shiv/3.7.2/html5shiv.min.js"></script>
    <script src="http://cdn.bootcss.com/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body>
<!-- 引用头 -->
<#--<jsp:include page="../page/header.jsp"></jsp:include>-->

<!-- 引用头 -->
<#include "page/header.ftl"/>

<div class="page_body container-fluid">
    <div class="row">
    <#include "page/left.ftl"/>

        <#--<div class="col-lg-20 col-md-19 col-sm-18 main">-->
            <#--<div class="main_cont">-->
            <#--<#include "page/breadcrumbs.ftl"/>-->

                <#--<div class="common_data" style="padding-left:20px;">-->

                <#--</div>-->

            <#--</div>-->
        <#--</div>-->
    </div>
</div>




<div class="page_body container-fluid">
    <div class="row">
    <#--<jsp:include page="../page/left.jsp"></jsp:include>-->
        <div class="col-lg-20 col-md-19 col-sm-18 main">
            <div class="main_cont">
            <#--<jsp:include page="../page/breadcrumbs.jsp"></jsp:include>-->

                <h2 class="main_title">平台商品列表</h2>

                <div class="common_data p20">
                    <div class="filter_area">
                        <input type="hidden" value="searchForm" id="formId">
                        <input type="hidden" value="findAllBrand.htm" id="formName">
                        <form role="form" class="form-inline" id="searchForm" action="findAllBrand.htm">
                            <input type="hidden" name="CSRFToken" id="CSRFToken" value="${token}">
                            <div class="form-group">
                                <div class="input-group">
                                    <span class="input-group-addon">商品名称</span>
                                    <input type="text" class="form-control" name="brandName" value="${selectBean.brandName}">
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="input-group">
                                    <span class="input-group-addon">商品编号</span>
                                    <input type="text" class="form-control" name="brandNickname" value="${selectBean.brandNickname}">
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="input-group">
                                    <span class="input-group-addon">商品分类</span>
                                    <input type="text" class="form-control" name="brandSort" value="${selectBean.brandSort}">
                                </div>
                            </div>
                            <div class="form-group">
                                <button type="submit" class="btn btn-primary">搜索</button>
                            </div>
                        </form>
                    </div>
                    <div class="data_ctrl_area mb20">
                        <div class="data_ctrl_search pull-right"></div>
                        <div class="data_ctrl_brns pull-left">
                        <#--<button type="button" class="btn btn-info" onclick="showEditBrandForm()">-->
                                <#--<i class="glyphicon glyphicon-plus"></i> 添加-->
                            <#--</button>-->
                            <#--<button type="button" class="btn btn-info" onclick="showDeleteBatchConfirmAlert('deleBrandForm','brandIds')">-->
                                <#--<i class="glyphicon glyphicon-trash"></i> 删除-->
                            <#--</button>-->
                        </div>
                        <div class="clr"></div>
                    </div>
                    <div class="table_tabs" id="good_tabs">
                        <ul>
                            <li class="<#if test="${searchBean.queryStatus == '1'}">active</#if>">
                                <a href="javascript:;" data-type="1">全部</a>
                            </li>
                            <li class="<#if test="${searchBean.queryStatus == '2'}"></#if>">
                                <a href="javascript:;" data-type="2">已审核</a>
                            </li>
                            <li class="<#if test="${searchBean.queryStatus == '3'}"></#if>">
                                <a href="javascript:;" data-type="3">待审核</a>
                            </li>
                        </ul>
                    </div>
                    <form id="deleBrandForm" action="batchDelBrand.htm?CSRFToken=${token }" method="post">
                        <input type="hidden" value="${pb.pageNo}" name="pageNo"/>
                        <table class="table table-striped table-hover">
                            <thead>
                            <tr>
                                <th width="10"><input type="checkbox" onclick="allunchecked(this,'brandIds')"></th>
                                <th>图片</th>
                                <th>货品名称</th>
                                <th>货品编号</th>
                                <th>货品规格</th>
                                <th>销售价</th>
                                <th>结算价</th>
                                <th>是否上架</th>
                                <th>所属商家</th>
                                <th>邮费</th>
                                <th>品牌</th>
                                <th width="150">操作</th>
                            </tr>
                            </thead>
                            <tbody>
                            <tr>
                                <td><input type="checkbox" name="brandIds" value="${brand.brandId }"/></td>
                                <td>
                                    <a href="" target="_blank">
                                        <div class="data_item">
                                            <img  height="60" src=''/>
                                        </div>
                                    </a>
                                </td>
                                <td>
                                    <span title="">商品名称1</span>
                                </td>
                                <td>
                                    <span>商品编号1</span>
                                </td>
                                <td>
                                    <span>规格</span>
                                </td>
                                <td>
                                    <span class="text-muted">销售价1</span>
                                </td>
                                <td>
                                    <span class="text-muted">结算价1</span>
                                </td>
                                <td>
                                <#if test="${goods.goodsAdded==0}"><a  title="点击上架" href="updateGoodsSta.htm?CSRFToken=${token }&goodsId=${goods.goodsId }&goodsAdded=1&pageNo=${pb.pageNo }"  href="javascript:;" class="label label-default">否</a></#if>
                                </td>
                                <td width="75">
                                    <span class="text-muted">所属商家1</span>
                                </td>
                                <td>
                                    <span class="text-muted">邮费1</span>
                                </td>
                                <td>
                                    <span class="text-muted">品牌1</span>
                                </td>
                                <td>
                                    <div class="btn-group">
                                        <button type="button" class="btn btn-default" onclick="showEditBrandForm(${brand.brandId})">审核</button>
                                        <button type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown">
                                            <span class="caret"></span>
                                            <span class="sr-only">Toggle Dropdown</span>
                                        </button>
                                        <ul class="dropdown-menu" role="menu">
                                            <li><a href="javascript:;" onclick="showDeleteOneConfirmAlert('delBrand.htm?CSRFToken=${token}&brandId=${brand.brandId}','确认要拒绝审核通过嘛${brand.brandName}吗')">拒绝</a></li>
                                        </ul>
                                    </div>
                                </td>
                            </tr>
                            </tbody>
                        </table>
                    </form>

                    <div class="table_foot">
                        <#include "page/searchPag.ftl"/>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


<div class="table_foot">
<#include "page/searchPag.ftl"/>
</div>

<!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
<!-- Include all compiled plugins (below), or include individual files as needed -->
<script src="${bath}/static/bootstrap/js/bootstrap.min.js"></script>
<script src="${bath}/static/js/summernote.min.js"></script>
<script src="${bath}/static/js/language/summernote-zh-CN.js"></script>
<script src="${bath}/static/bootstrap/js/bootstrap-select.min.js"></script>
<script src="${bath}/static/js/common.js"></script>
<script src="${bath}/static/js/common/common_alert.js"></script>
<script src="${bath}/static/js/common/common_checked.js"></script>
<script src="${bath}/static/js/ThirdPlantManage/PlantGoodsList.js"></script>

</body>
</html>









