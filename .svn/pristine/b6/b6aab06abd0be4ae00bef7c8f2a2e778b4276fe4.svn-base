    <select id="getBillDataFromReplementBill" resultMap="BaseResultMap" parameterType="java.lang.Long">
        SELECT
        chinapost_inventory_bill.bill_id,
        chinapost_inventory_bill.code,
        chinapost_inventory_bill.creator_id as in_id,
        chinapost_inventory_bill_item.item_id,
        chinapost_inventory_bill_item.goods_info_id,
        chinapost_inventory_bill_item.amount,
        chinapost_inventory_bill_item.checked_amount,
        yei2.enterprise_name AS in_name,
        yei2.linkman,
        yei2.link_mobile,
        yei2.address,
        yei1.enterprise_id as out_id,
        yei1.enterprise_name AS out_name,
        inter_goods_info.available,
        inter_goods_info.goods_info_item_no,
        inter_goods_info.goods_info_name,
        inter_goods_info.spec_string,
        inter_goods_info.third_name,
        inter_goods_info.third_id,
        inter_goods_info.goods_info_settle_price
        FROM
        chinapost_inventory_bill
        LEFT JOIN chinapost_inventory_bill_item ON chinapost_inventory_bill.bill_id =
        chinapost_inventory_bill_item.bill_id
        LEFT JOIN ysh_enterprise_info AS yei2 ON yei2.enterprise_id = chinapost_inventory_bill.creator_id
        LEFT JOIN ysh_enterprise_info AS yei1 ON yei1.enterprise_id = #{outId}
        LEFT JOIN (
        SELECT
        np_goods_info.goods_info_id,
        np_goods_info.goods_info_name,
        np_goods_info.goods_info_item_no,
        np_goods_info.third_name,
        np_goods_info.third_id,
        IFNULL(np_goods_info.goods_info_settle_price,0) as goods_info_settle_price,
        ifnull(
        group_concat(
        concat(
        np_goods_spec.spec_name,
        ":",
        ifnull(
        np_goods_spec_detail.spec_detail_name,
        ""
        )
        )
        ),
        ""
        ) AS spec_string,
        IFNULL(
        ysh_enterprise_inventory.available,
        0
        ) AS available
        FROM
        np_goods_info
        LEFT JOIN np_goods_info_rele_spec_detail ON np_goods_info_rele_spec_detail.goods_info_id =
        np_goods_info.goods_info_id
        LEFT JOIN np_goods_spec ON np_goods_spec.spec_id = np_goods_info_rele_spec_detail.spec_id
        LEFT JOIN np_goods_spec_detail ON np_goods_spec_detail.spec_detail_id =
        np_goods_info_rele_spec_detail.spec_detail_id
        LEFT JOIN ysh_enterprise_inventory ON np_goods_info.goods_info_id = ysh_enterprise_inventory.goods_info_id
        AND ysh_enterprise_inventory.enterprise_id =#{outId}
        WHERE
        np_goods_info.goods_info_id IN (
        SELECT
        chinapost_inventory_bill_item.goods_info_id
        FROM
        chinapost_inventory_bill_item
        WHERE
        chinapost_inventory_bill_item.bill_id = #{billId,jdbcType=BIGINT}
        )
        GROUP BY
        goods_info_id
        ) AS inter_goods_info ON chinapost_inventory_bill_item.goods_info_id = inter_goods_info.goods_info_id
        WHERE
        chinapost_inventory_bill.bill_id =#{billId,jdbcType=BIGINT}
    </select>

