    <select id="selectBillDetailByPrimaryKey" resultMap="BaseResultMap" parameterType="java.lang.Long">
        SELECT
        <include refid="Base_Column_List"/>,
        <include refid="Base_billItem_Column_List"/>,
        <include refid="Base_billLog_Column_List"/>,
        yei5.enterprise_name AS current_name,
        yei4.enterprise_name AS creator_name,
        yei3.enterprise_name AS operator_name,
        yei2.enterprise_name AS in_name,
        yei2.linkman ,
        yei2.link_mobile ,
        yei2.address ,
        yei1.enterprise_name AS out_name,
        inter_goods_info.available,
        inter_goods_info.goods_info_item_no,
        inter_goods_info.goods_info_name,
        inter_goods_info.goods_info_type,
        inter_goods_info.spec_string,
        inter_goods_info.third_name,
        inter_goods_info.third_id,
        inter_goods_info.goods_info_settle_price
        FROM
        chinapost_inventory_bill
        LEFT JOIN chinapost_inventory_bill_item ON chinapost_inventory_bill.bill_id =
        chinapost_inventory_bill_item.bill_id
        LEFT JOIN chinapost_inventory_bill_log ON chinapost_inventory_bill.bill_id =
        chinapost_inventory_bill_log.bill_id
        LEFT JOIN ysh_enterprise_info AS yei5 ON yei5.enterprise_id = chinapost_inventory_bill.current_id
        LEFT JOIN ysh_enterprise_info AS yei4 ON yei4.enterprise_id = chinapost_inventory_bill.creator_id
        LEFT JOIN ysh_enterprise_info AS yei3 ON yei3.enterprise_id = chinapost_inventory_bill_log.operator_id
        LEFT JOIN ysh_enterprise_info AS yei2 ON yei2.enterprise_id = chinapost_inventory_bill.in_id
        LEFT JOIN ysh_enterprise_info AS yei1 ON yei1.enterprise_id = chinapost_inventory_bill.out_id
        LEFT JOIN (
        SELECT
        np_goods_info.goods_info_id,
        np_goods_info.goods_info_name,
        np_goods_info.goods_info_item_no,
        np_goods_info.goods_info_type,
        np_goods_info.third_name,
        np_goods_info.third_id,
        IFNULL(np_goods_info.goods_info_settle_price,0) as goods_info_settle_price,
        ifnull(
        group_concat(
        concat(
        np_goods_spec.spec_name,
        ":",
        ifnull(
        np_goods_spec_detail.spec_detail_name,
        ""
        )
        )
        ),
        ""
        ) AS spec_string,
        IFNULL(
        ysh_enterprise_inventory.available,
        0
        ) AS available
        FROM
        np_goods_info
        LEFT JOIN np_goods_info_rele_spec_detail ON np_goods_info_rele_spec_detail.goods_info_id =
        np_goods_info.goods_info_id
        LEFT JOIN np_goods_spec ON np_goods_spec.spec_id = np_goods_info_rele_spec_detail.spec_id
        LEFT JOIN np_goods_spec_detail ON np_goods_spec_detail.spec_detail_id =
        np_goods_info_rele_spec_detail.spec_detail_id
        LEFT JOIN ysh_enterprise_inventory ON np_goods_info.goods_info_id = ysh_enterprise_inventory.goods_info_id
        AND ysh_enterprise_inventory.enterprise_id = (
        SELECT
        creator_id
        FROM
        chinapost_inventory_bill
        WHERE
        bill_id = #{billId,jdbcType=BIGINT}
        )
        WHERE
        np_goods_info.goods_info_id IN (
        SELECT
        chinapost_inventory_bill_item.goods_info_id
        FROM
        chinapost_inventory_bill_item
        WHERE
        chinapost_inventory_bill_item.bill_id = #{billId,jdbcType=BIGINT}
        )
        GROUP BY
        goods_info_id
        ) AS inter_goods_info ON chinapost_inventory_bill_item.goods_info_id = inter_goods_info.goods_info_id
        WHERE
        chinapost_inventory_bill.bill_id = #{billId,jdbcType=BIGINT}
        ORDER BY chinapost_inventory_bill_log.create_time DESC
    </select>


    <select id="selectByCreatorId" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List"/>,
        yei1.enterprise_name as creator_name,
        yei2.enterprise_name as current_name
        from chinapost_inventory_bill
        left join ysh_enterprise_info as yei1 on yei1.enterprise_id = chinapost_inventory_bill.creator_id
        left join ysh_enterprise_info as yei2 on yei2.enterprise_id = chinapost_inventory_bill.current_id
        <where>
            creator_id = #{creatorId,jdbcType=BIGINT}
            <if test="code!=null">
                and
                chinapost_inventory_bill.code=#{code,jdbcType=BIGINT}
            </if>
            <if test="creatorName!=null">
                and
                yei1.enterprise_name like concat(concat('%', #{creatorName,jdbcType=VARCHAR}),'%')
            </if>
            <if test="billType != null">
                and bill_type = #{billType,jdbcType=VARCHAR}
            </if>
            <if test="billStatus != null">
                and bill_status = #{billStatus,jdbcType=VARCHAR}
            </if>
            <if test="end!=null">
                <![CDATA[
                and
                  chinapost_inventory_bill.create_time<= #{end,jdbcType=TIMESTAMP}
                  ]]>
            </if>
            <if test="start!=null">
                <![CDATA[
                  and
                  chinapost_inventory_bill.create_time>=#{start,jdbcType=TIMESTAMP}
            ]]>
            </if>
        </where>
        order by chinapost_inventory_bill.create_time desc
        <if test="pageable != null">
            limit
            #{pageable.index,jdbcType=BIGINT}, #{pageable.length,jdbcType=BIGINT}
        </if>
    </select>

