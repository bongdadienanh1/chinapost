$(function() {
    $("#queryStatus").val($("#good_tabs").find(".active").find("a").attr("data-type"));


    /*收款单筛选交互演示用，无实际意义*/
    $('#good_tabs a').click(function () {
        $that = $(this);
        $that.parent().addClass('active');
        $("#queryStatus").val($that.attr("data-type"));
        $that.parent().siblings().removeClass('active');
        $('#adv_form').submit();

    });
});