public enum BillType {

    //请货
    REPLENISHMENT("补货"),

    MERGE_REPLENISHMENT("合并的补货"),
    //报损
    LESS_REPORT("报损"),
    //报溢
    MORE_REPORT("报溢"),
    //调拨
    INVENTORY_TRANSFER("调拨"),

    INVENTORY_INSTOCK("入库"),

    PURCHASE_INVENTORY_INSTOCK("采购入库");

